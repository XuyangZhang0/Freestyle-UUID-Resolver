{
  "manifest_version": 3,
  "name": "UUID Resolver - DEBUG",
  "version": "1.0.1",
  "description": "Debug version for UUID resolution",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://na1.data.workspaceone.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://na1.data.workspaceone.com/*"
      ],
      "js": [
        "content/debug-test.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background/service-worker.js"
  },
  "action": {
    "default_title": "UUID Resolver DEBUG"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
